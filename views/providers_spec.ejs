<table>
    <thead>
        <tr> 
            <th>First Name</th>
            <th>Last Name</th>
            <th>Provider_Specialty</th>
            <th>Email Address</th>
            <th>Phone Number</th>
            <th>Date Joined</th>
           
            <!-- <th>Date of Birth</th> -->

        </tr>   

    </thead>
    <tbody>
        <% results.forEach(row =>{ %>
        <tr>
            <td><%= row.first_name %></td>
             <td><%= row.last_name %></td> 
            <td><%= row.provider_specialty %></td>
            <td><%= row.email_address %></td>
            <td><%= row.phone_number %></td>
            <td><%= row.date_joined %></td>
            
            <!-- <td><%= row.date_of_birth %></td> --> 
        </tr>
        <% }); %>
    </tbody>
</table>